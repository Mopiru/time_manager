ent();\n      let data = {\n        workingTimes: {\n          time: datetime.format(),\n          status: false,\n          user: this.userId,\n        },\n      };\n      const res = await axios.post(request, JSON.stringify(data));\n    },\n  },\n};\n</script>\n\n\n<style scoped>\n#comp-cont {\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n  align-items: flex-start;\n}\n\n.title {\n  font: 20px \"montserrat\";\n  margin-bottom: 15px;\n}\n\n.timer-butt {\n  height: 43px;\n  padding: 0px 10px;\n  border-radius: 40px;\n  border: none;\n  outline: none;\n  cursor: pointer;\n  box-shadow: 0px 0px 7px 7px rgba(255, 255, 255, 0.16);\n  font: 16px \"montserrat\";\n  cursor: pointer;\n}\n\n.timer-butt:hover {\n  background-color: seagreen;\n}\n\n.start-timer-butt {\n  margin-right: 15px;\n}\n</style>"]}]}